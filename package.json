{
    "name": "darktheme",
    "displayName": "DarkTheme",
    "description": "A Utilitarian Dark-Mode Theme",
    "version": "0.0.1",
    "author": "Nathan Chung",
    "publisher": "Nathan13888",
    "license": "AGPL-3.0",
    "icon": "icon.png",
    "scripts": {
        "build": "npm run build:dark && npm run build:bright",
        "build:dark": "node src/dark/build",
        "build:bright": "node src/bright/build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Nathan13888/VSCodeTheme"
    },
    "bugs": {
        "url": "https://github.com/Nathan13888/VSCodeTheme/issues"
    },
    "engines": {
        "vscode": "^1.43.0"
    },
    "galleryBanner": {
        "color": "#FCEAE5",
        "theme": "light"
    },
    "keywords": [
        "theme",
        "themes",
        "color",
        "colour",
        "dark"
    ],
    "categories": [
        "Themes"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "themes": [
            {
                "label": "DarkTheme",
                "uiTheme": "vs-dark",
                "path": "./themes/darktheme.json"
            },
            {
                "label": "DarkTheme Italic",
                "uiTheme": "vs-dark",
                "path": "./themes/darktheme-italic.json"
            },
            {
                "label": "DarkTheme Bold",
                "uiTheme": "vs-dark",
                "path": "./themes/darktheme-bold.json"
            },
            {
                "label": "DarkTheme Bright",
                "uiTheme": "vs",
                "path": "./themes/darktheme-bright.json"
            },
            {
                "label": "DarkTheme Bright Italic",
                "uiTheme": "vs",
                "path": "./themes/darktheme-bright-italic.json"
            },
            {
                "label": "DarkTheme Bright Bold",
                "uiTheme": "vs",
                "path": "./themes/darktheme-bright-bold.json"
            }
        ],
        "commands": [],
        "configuration": {
            "title": "Dark Theme",
            "properties": {}
        }
    },
    "devDependencies": {
        "mustache": "^3.0.1"
    }
}
